<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PlayerServlet</servlet-name>
    <servlet-class>it.contrader.servlets.PlayerServlet</servlet-class>
  </servlet>
 
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>4000</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>UserServlet</display-name>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>it.contrader.servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>StatsboxServlet</servlet-name>
  	<servlet-class>it.contrader.servlets.StatsboxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StatsboxServlet</servlet-name>
  	<url-pattern>/StatsboxServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>PlayerServlet</servlet-name>
  	<url-pattern>/PlayerServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<description></description>
  	<display-name>SearchServlet</display-name>
  	<servlet-name>SearchServlet</servlet-name>
  	<servlet-class>it.contrader.servlets.SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/SearchServlet</url-pattern>
  </servlet-mapping>
   <servlet>
  	<description></description>
  	<display-name>TeamServlet</display-name>
  	<servlet-name>TeamServlet</servlet-name>
  	<servlet-class>it.contrader.servlets.TeamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TeamServlet</servlet-name>
  	<url-pattern>/TeamServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<description></description>
  	<display-name>InjuryServlet</display-name>
  	<servlet-name>InjuryServlet</servlet-name>
  	<servlet-class>it.contrader.servlets.InjuryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InjuryServlet</servlet-name>
  	<url-pattern>/InjuryServlet</url-pattern>
  </servlet-mapping>
</web-app>